        function OnConfirmDeleteDocument(id_) {
            Swal.fire({
                title: 'Are you sure want to delete this document?',
                text: "You won't be able to revert this!",
                icon: "warning",
                showCancelButton: !0,
                confirmButtonColor: "#28bb4b",
